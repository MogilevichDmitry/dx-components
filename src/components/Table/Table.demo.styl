@require './Table.styl';

.container {
	border-collapse: separate;
	width: 100%;
	
	background-color: #161412;
}

.head {
	background-color: #242424;
	color: #FFFFFF;
}
	
.body {
	color: #999999;
}
	
.cell {
	vertical-align: middle;
	text-align: center;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
	padding: 10px;
}