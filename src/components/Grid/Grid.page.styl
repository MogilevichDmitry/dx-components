.cell {
	box-shadow: inset 0 0 1px grey;
	&_first {
		composes: cell;
		width: 500px;
	}
	&_second {
		composes: cell;
		width: 400px;
	}
	&_third {
		composes: cell;
		width: 600px;
	}
	&_second_third {
		composes: cell;
		width: 400px + 600px;
	}
	&_first_second {
		composes: cell;
		width: 500px + 400px;
	}
}